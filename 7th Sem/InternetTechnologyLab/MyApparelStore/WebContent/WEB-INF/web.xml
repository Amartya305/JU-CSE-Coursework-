<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet> 
<servlet-name>Signup</servlet-name> 
<servlet-class>com.achoudhury.SignupServlet</servlet-class>
</servlet>
<servlet> 
<servlet-name>Login</servlet-name> 
<servlet-class>com.achoudhury.Login</servlet-class>
</servlet>
<servlet> 
<servlet-name>Logout</servlet-name> 
<servlet-class>com.achoudhury.Logout</servlet-class>
</servlet>
<servlet>
<servlet-name>Welcome</servlet-name>
<servlet-class>com.achoudhury.Welcome</servlet-class>
</servlet>    
<servlet-mapping> 
<servlet-name>Signup</servlet-name> 
<url-pattern>/Signup</url-pattern>
</servlet-mapping>
<servlet-mapping> 
<servlet-name>Login</servlet-name> 
<url-pattern>/Login</url-pattern>
</servlet-mapping>
<servlet-mapping> 
<servlet-name>Logout</servlet-name> 
<url-pattern>/Logout</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>Welcome</servlet-name>
<url-pattern>/Welcome</url-pattern>
</servlet-mapping>
</web-app>